{
	"name": "vari-wordle",
	"private": true,
	"version": "1.2.0",
	"type": "module",
	"scripts": {
		"docker-start": "docker-compose --env-file .env up -d",
		"docker-build": "docker build -t vari-wordle-dev .",
		"dev": "concurrently \"ts-node-esm server/index.ts\" \"vite\"",
		"build": "tsc && vite build",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"test:unit": "vitest --root src/",
		"start": "ts-node-esm -T server/index.ts",
		"backend": "ts-node-esm server/index.ts",
		"frontend": "vite"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.9.5",
		"axios": "^1.4.0",
		"chart.js": "^4.3.0",
		"cors": "^2.8.5",
		"dotenv": "^16.1.4",
		"express": "^4.18.2",
		"firebase": "^9.22.2",
		"firebase-admin": "^11.9.0",
		"logrocket": "^4.0.3",
		"logrocket-react": "^5.0.1",
		"node-schedule": "^2.1.1",
		"random-words": "^2.0.0",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.10",
		"react-icons": "^4.9.0",
		"react-redux": "^8.1.0",
		"redux": "^4.2.1",
		"redux-persist": "^6.0.0",
		"use-dark-mode": "^2.3.1"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@types/chart.js": "^2.9.37",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/logrocket-react": "^3.0.0",
		"@types/node": "^20.3.1",
		"@types/node-schedule": "^2.1.0",
		"@types/react": "^18.0.37",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"@vitejs/plugin-react-swc": "^3.0.0",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"eslint": "^8.38.0",
		"eslint-plugin-react-hooks": "^4.6.0",
