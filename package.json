		"jsdom": "^22.1.0",
		"nodemon": "^2.0.22",
		"postcss": "^8.4.24",
		"tailwindcss": "^3.3.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.2",
		"vite": "^4.3.9",
		"vitest": "^0.32.2"
	},
